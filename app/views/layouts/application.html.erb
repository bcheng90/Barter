<!DOCTYPE html>
<html>
<head>
  <title>MyGreatApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if current_user %>
 <%= current_user.username %>
 <%= link_to "Logout", logout_path %>
<% else %>
  <%= link_to "Login", login_path %>
  <%= link_to "Signup", signup_path %>
<% end %>
<%= yield %>

</body>
</html>
